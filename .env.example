# General
NODE_ENV=
APPOINTMENT_TIMEZONE=America/New_York # optional; defaults to Eastern (America/New_York) if unset
E2E_RUN_ID=

# Site
NEXT_PUBLIC_SITE_URL=
NEXT_PUBLIC_GA4_ID=
NEXT_PUBLIC_META_PIXEL_ID=
NEXT_PUBLIC_API_BASE_URL=
# Public site branding (build-time; keep the marketing site fast and cacheable)
NEXT_PUBLIC_COMPANY_NAME=
NEXT_PUBLIC_COMPANY_PHONE_E164=
NEXT_PUBLIC_COMPANY_PHONE_DISPLAY=
NEXT_PUBLIC_COMPANY_EMAIL=
NEXT_PUBLIC_COMPANY_LOGO_PATH=
NEXT_PUBLIC_COMPANY_SERVICE_AREA=
NEXT_PUBLIC_COMPANY_HOURS_SUMMARY=
NEXT_PUBLIC_COMPANY_HQ_CITY=
NEXT_PUBLIC_COMPANY_HQ_STATE=
NEXT_PUBLIC_COMPANY_HQ_COUNTRY=US
NEXT_PUBLIC_GOOGLE_BUSINESS_PROFILE_URL=
NEXT_PUBLIC_FACEBOOK_PAGE_URL=
NEXT_PUBLIC_INSTAGRAM_URL=

# API Access
API_BASE_URL=http://localhost:3001
ADMIN_API_KEY=
CORS_ALLOW_ORIGINS=

# Analytics (server-side)
GA4_MEASUREMENT_ID=
GA4_API_SECRET=

# Database
DATABASE_URL=

# OpenAI (Chat)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-mini
OPENAI_INBOX_SUGGEST_MODEL=

# Google Ads / Analytics
# Google Ads tag ID (e.g. AW-123456789)
NEXT_PUBLIC_GOOGLE_ADS_TAG_ID=
# Lead/quote conversion "send_to" string (e.g. AW-123456789/AbCdEfGhIjK)
NEXT_PUBLIC_GOOGLE_ADS_LEAD_SEND_TO=
# Conversion "send_to" string (e.g. AW-123456789/AbCdEfGhIjK)
NEXT_PUBLIC_GOOGLE_ADS_CONTACT_SEND_TO=

# Instant Quote / Booking
# Promo discount applied to instant quote price ranges (e.g. 0.15 = 15% off).
INSTANT_QUOTE_DISCOUNT=0.15
# Max number of simultaneous appointments allowed (1 = no double-booking, 2 = two trailers, etc.)
APPOINTMENT_CAPACITY=

# Stripe
STRIPE_SECRET_KEY=

# Google Calendar
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REFRESH_TOKEN=
GOOGLE_CALENDAR_ID=
GOOGLE_CALENDAR_WEBHOOK_URL=
GOOGLE_CALENDAR_SYNC_LOOKBACK_DAYS=45

# Google Ads API (reporting)
# Create these in Google Cloud + Google Ads API center, then generate a refresh token with the googleads scope.
GOOGLE_ADS_DEVELOPER_TOKEN=
GOOGLE_ADS_CLIENT_ID=
GOOGLE_ADS_CLIENT_SECRET=
GOOGLE_ADS_REFRESH_TOKEN=
# Google Ads customer ID (no dashes preferred).
GOOGLE_ADS_CUSTOMER_ID=
# Optional: manager account customer ID (MCC) if your developer token requires it.
GOOGLE_ADS_LOGIN_CUSTOMER_ID=
# Optional: API version (default v17).
# Google Ads API releases deprecate older versions regularly.
# As of Jan 2026, v17 is no longer served by googleads.googleapis.com.
GOOGLE_ADS_API_VERSION=v20
# Optional: how often the outbox worker auto-queues a sync (ms). Default is 24h.
GOOGLE_ADS_SYNC_INTERVAL_MS=
# Optional: set to 1 to disable auto-sync.
GOOGLE_ADS_SYNC_DISABLED=

# Facebook/Messenger Webhook
FB_VERIFY_TOKEN=
FB_APP_SECRET=
FB_LEADGEN_ACCESS_TOKEN=
FB_MESSENGER_ACCESS_TOKEN=
FB_LEAD_FORM_IDS=
FB_PAGE_ID=
FB_PAGE_ACCESS_TOKEN=
META_DATASET_ID=
META_CONVERSIONS_TOKEN=
META_LEAD_EVENT_SOURCE=StonegateOS
FB_AD_ACCOUNT_ID=
FB_MARKETING_ACCESS_TOKEN=

# Notifications (SMS/Email)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM=
TWILIO_API_BASE_URL=
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
SMTP_FROM=

# Direct Message (DM) webhook
DM_WEBHOOK_URL=
DM_WEBHOOK_TOKEN=
DM_WEBHOOK_FROM=

# Internal Alerts
QUOTE_ALERT_EMAIL=
LEAD_ALERT_SMS=

# Outbox Worker
OUTBOX_BATCH_SIZE=10
OUTBOX_POLL_INTERVAL_MS=0
SEO_AUTOPUBLISH_INTERVAL_MS=
SEO_AUTOPUBLISH_DISABLED=
